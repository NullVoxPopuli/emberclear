  "license": "GPL-3.0",
  "author": "NullVoxPopuli",
  "main": "src/main.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/NullVoxPopuli/emberclear",
  "scripts": {
    "ember": "EMBER_CLI_BROCCOLI_2=true EMBER_CLI_SYSTEM_TEMP=true EMBER_CLI_MODULE_UNIFICATION=true node_modules/.bin/ember",
    "ember:update": "node_modules/.bin/ember-cli-update",
    "eslint": "node_modules/.bin/eslint",
    "tsc": "node_modules/.bin/tsc",
    "prettier": "node_modules/.bin/prettier",
    "build": "yarn ember build",
    "build:debug": "EMBER_CLI_PACKAGER=true BROCCOLI_DEBUG=* yarn build",
    "build:production": "ENABLE_SW=true PREMBER=true EXPERIMENTAL_RENDER_MODE_SERIALIZE=true yarn build --environment production && sed -i -e 's/{{ROOT_URL}}/\\//g' dist/index.html",
    "build:production:analyze": "CONCAT_STATS=true yarn build:production",
    "start": "yarn ember serve -p 4201",
    "start:dev": "ENABLE_SW=false SW_DISABLED=true FASTBOOT_DISABLED=true yarn start -p 4201",
    "test": "yarn ember exam --random",
    "test:server": "yarn ember exam --server",
    "lint:hbs": "node_modules/.bin/ember-template-lint .",
    "lint:js": "yarn eslint './**/*.ts'",
    "lint:pretty": "yarn prettier \"{config,lib,server,tests/**/*.ts,src/**/*.ts}\"",
    "lint:fix": "bash -c 'yarn lint --fix'",
    "deploy": "yarn ember deploy production",
    "bundle-analyze": "./node_modules/.bin/broccoli-concat-analyser ./concat-stats-for",
    "analyze": "yarn build:production:analyze && yarn bundle-analyze && cp ./concat-stats-for/index.html ./public/bundle.html"
  },
