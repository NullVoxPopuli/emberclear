<FocusCard @title={{t 'ui.setup.overwriteTitle'}}>
  <p>{{t 'ui.setup.overwriteQuestion' htmlSafe=true}}</p>

  <div class='cta-with-fallback'>
    <button
      data-test-overwrite-abort
      type='button'
      class='card-footer-item'
      {{on  'click' this.didClickNo}}
    >
      {{t 'ui.setup.overwriteAbort'}}
    </button>

    <button
      data-test-overwrite-confirm
      type='button'
      class='card-footer-item'
      {{on  'click' this.didClickYes}}
    >
      {{t 'ui.setup.overwriteConfirm'}}
    </button>
  </div>
</FocusCard>
