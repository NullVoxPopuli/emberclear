<a
  href={{url-for @to @id}}
  {{on 'click' (prevent-default
    (queue
      (transition-to (url-for @to @id))
      @afterSelect
    )
  )}}
  class='
    flex-row justify-content-space-between
    p-sm
    has-border-radius
    has-text-dark has-hover-highlight
  '
  ...attributes
>
  {{yield}}
</a>
