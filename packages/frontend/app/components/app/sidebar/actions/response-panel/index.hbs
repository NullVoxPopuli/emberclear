<button class='button button-xs' type='button' data-test-action-response-yes {{on 'click' this.yes}}>
	<span class='icon'>{{fa-icon 'check'}}</span>
</button>

<button class='button button-xs' type='button' data-test-action-response-no {{on 'click' this.no}}>
	<span class='icon'>{{fa-icon 'times'}}</span>
</button>

<button class='button button-xs' type='button' data-test-action-response-dismiss {{on 'click' this.dismiss}}>
	<span class='icon'>{{fa-icon 'minus'}}</span>
</button>