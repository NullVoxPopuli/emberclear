<a {{on 'click' (prevent-default this.yes())}}>
	<span class='icon'>{{fa-icon 'check'}}</span>
</a>

<a {{on 'click' (prevent-default this.no())}}>
	<span class='icon'>{{fa-icon 'remove'}}</span>
</a>

<a {{on 'click' (prevent-default this.dismiss())}}>
	<span class='icon'>{{fa-icon 'minus'}}</span>
</a>