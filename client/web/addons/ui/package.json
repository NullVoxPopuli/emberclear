{
  "name": "@emberclear/ui",
  "version": "0.0.0",
  "description": "Base ui components for emberclear.io",
  "private": true,
  "keywords": [
    "ember-addon",
    "emberclear"
  ],
  "repository": {
    "url": "https://github.com/NullVoxPopuli/emberclear",
    "directory": "client/web/addons/ui"
  },
  "license": "GPL-3.0",
  "author": "NullVoxPopuli",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint . --ext js,ts",
    "start": "ember serve",
    "test": "ember test",
    "prepublish": "tsc --build"
  },
  "dependencies": {
    "@fortawesome/ember-fontawesome": "0.2.3",
    "@fortawesome/free-brands-svg-icons": "5.15.2",
    "@fortawesome/free-solid-svg-icons": "5.15.2",
    "@glimmer/component": "1.0.3",
    "@glimmer/tracking": "1.0.3",
    "ember-auto-import": "1.10.1",
    "ember-cli-babel": "7.23.1",
    "ember-cli-htmlbars": "5.3.2",
    "ember-cli-postcss": "7.0.0",
    "ember-focus-trap": "0.5.0",
    "ember-keyboard": "6.0.2",
    "ember-named-blocks-polyfill": "0.2.4",
    "ember-tracked-local-storage": "*",
    "enhanced-resolve": "5.7.0",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "14.0.0",
    "shoelace-css": "1.0.0-beta24",
    "typescript": "4.2.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@ember/optional-features": "2.0.0",
    "@emberclear/config": "*",
    "@emberclear/eslint": "*",
    "@emberclear/questionably-typed": "*",
    "@types/ember": "3.16.3",
    "@types/ember-qunit": "3.4.13",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__test-helpers": "1.7.3",
    "@types/qunit": "2.11.1",
    "@types/rsvp": "4.0.3",
    "broccoli-asset-rev": "3.0.0",
    "ember-auto-import": "1.10.1",
    "ember-cli": "3.25.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-inject-live-reload": "2.0.2",
    "ember-cli-page-object": "1.17.4",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.1",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-named-blocks-polyfill": "0.2.4",
    "ember-qunit": "4.6.0",
    "ember-resolver": "8.0.2",
    "ember-source": "3.25.1",
    "loader.js": "4.7.0",
    "npm-run-all": "4.1.5",
    "qunit-dom": "1.6.0",
    "testem-failure-only-reporter": "0.0.1",
    "typescript": "4.2.2"
  },
  "engines": {
    "node": "14.15.5"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "before": [
      "ember-cli-postcss"
    ],
    "configPath": "tests/dummy/config"
  },
  "typesVersions": {
    "*": {
      "*": [
        "declarations/*",
        "declarations/*/index"
      ]
    }
  },
  "volta": {
    "node": "14.15.5",
    "yarn": "1.22.10"
  }
}
