  "name": "emberclear",
  "version": "0.0.1",
  "private": false,
  "description": "Small description for emberclear goes here",
  "license": "GPL-3.0",
  "author": "NullVoxPopuli",
  "main": "app/app.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
