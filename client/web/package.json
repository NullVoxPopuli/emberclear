{
  "name": "emberclear-web",
  "version": "0.0.0",
  "description": "PWA client for emberclear",
  "main": "index.js",
  "repository": "https://github.com/NullVoxPopuli/emberclear",
  "author": "NullVoxPopuli",
  "license": "GPL-3.0",
  "private": true,
  "workspaces": [
    "emberclear",
    "smoke-tests",
    "questionably-typed/*",
    "config",
    "addons/*",
    "libraries/*",
    "lint/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "yarn eslint --fix --quiet --cache",
    "*.hbs": "yarn ember-template-lint --fix",
    "*.css": "yarn stylelint --fix --cache",
    "translations/*.yml": "yarn ember-intl-analyzer"
  },
  "resolutions": {
    "ember-test-waiters": "2.1.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "3.4.0",
    "@typescript-eslint/parser": "3.4.0",
    "babel-eslint": "10.1.0",
    "cross-env": "7.0.2",
    "deepmerge": "4.2.2",
    "ember-intl-analyzer": "2.1.0",
    "ember-template-lint": "2.8.0",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-decorator-position": "2.1.0",
    "eslint-plugin-ember": "8.8.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-qunit": "4.2.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "sass-lint": "1.13.1",
    "stylelint": "13.6.1",
    "stylelint-config-standard": "20.0.0"
  },
  "engines": {
    "node": "14.4.0"
  },
  "volta": {
    "node": "14.4.0",
    "yarn": "1.22.4"
  }
}
